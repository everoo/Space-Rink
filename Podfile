# platform :ios, '9.0'

target 'Space Arena' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  # Pods for Space Arena
	pod 'Google-Mobile-Ads-SDK'
	pod 'Firebase/Core'
	pod 'Firebase/Auth'
	pod 'Firebase/AdMob'
	pod 'Firebase/Messaging'
	pod 'Firebase/Database'
	pod 'Firebase/Crash'
	pod 'Firebase/Storage'
	
  target 'Space ArenaTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'Space ArenaUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
